generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model News {
  id         String   @id @default(cuid())
  title      String
  url        String
  type       String
  updateTime DateTime @updatedAt

  @@index([title, updateTime(sort: Desc)])
}
