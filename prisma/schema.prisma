generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(cuid())
  name     String @unique
  password String
}

model Article {
  id   String @id @default(cuid())
  name String
  body Bytes
}
